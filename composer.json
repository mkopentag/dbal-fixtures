{
    "name": "opentag/dbal-fixtures",
    "description": "Fixtures management with Doctrine DBAL",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Montealegre",
            "email": "montealegreluis@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "doctrine/dbal": "^2.5",
        "symfony/console": "^5.0",
        "symfony/yaml": "^5.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "ComPHPPuebla\\Fixtures\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ComPHPPuebla\\Fixtures\\": [
                "tests/integration",
                "tests/src",
                "tests/unit"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "chmod u+x bin/cli"
        ],
        "post-update-cmd": [
            "chmod u+x bin/cli"
        ]
    }
}
